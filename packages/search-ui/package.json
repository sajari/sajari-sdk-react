{
  "name": "@sajari/react-search-ui",
  "version": "1.0.0-alpha.33",
  "description": "Easy to use search components to quickly build a beautiful search interface powered by Sajari.",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "directory": "packages/search-ui"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "jest",
    "test:ci": "jest --env=jsdom --coverage --watchAll=false --maxWorkers=2",
    "prepublish": "yarn build"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@sajari/react-components": "^1.0.0-alpha.26",
    "@sajari/react-hooks": "^1.0.0-alpha.22",
    "@sajari/react-sdk-utils": "^1.0.0-alpha.14",
    "i18next": "19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "react-aria-live": "^2.0.5",
    "react-i18next": "^11.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
